{
  "name": "userauth",
  "version": "0.0.1",
  "description": "An example project to user authentication with node-express and react.",
  "main": "index.js",
  "scripts": {
    "api": "node ./src/api/api.js",
    "clean": "rm -rf ./dist/*",
    "lint": "eslint --max-warnings 0 --ext .jsx,.js src/ ",
    "prod": "npm run lint && webpack-dev-server --env=prod",
    "dev": "npm run clean && webpack-dev-server --env=dev",
    "dist:prod": "npm run clean && webpack -p --env=prod && cd .. && tar -czf userauth.tar.gz userauth",
    "dist:dev": "npm run clean && webpack --env=dev && cd .. && tar -czf userauth.tar.gz userauth"
  },
  "author": "Mustafa Arikan",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "css-loader": "^0.28.4",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-loader": "1.7.1",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "yargs": "^8.0.1"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "babel-preset-stage-1": "^6.1.18",
    "body-parser": "^1.17.2",
    "crypto-js": "^3.1.9-1",
    "express": "https://registry.npmjs.org/express/-/express-4.15.3.tgz",
    "jsonfile": "^3.0.0",
    "mustache": "^2.3.0",
    "postmark": "^1.3.1",
    "prop-types": "^15.5.10",
    "qs": "^6.4.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  }
}
